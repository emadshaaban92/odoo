<?xml version="1.0"?>
<odoo>
    <!-- note Stage Form View -->
    <record id="view_note_stage_form" model="ir.ui.view">
      <field name="name">nse.demo.form</field>
      <field name="model">nse.demo</field>
      <field name="arch" type="xml">
        <form>
          <group>
            <field name="name"/>
          </group>
        </form>
      </field>
    </record>

    <!-- note Stage Tree View -->
    <record id="view_note_stage_tree" model="ir.ui.view">
      <field name="name">nse.demo.tree</field>
      <field name="model">nse.demo</field>
      <field name="field_parent"></field>
      <field name="arch" type="xml">
        <tree string="NSE">
            <field name="name"/>
        </tree>
      </field>
    </record>

  <!-- Action -->
    <record id="action_nse" model="ir.actions.act_window">
      <field name="name">NSE</field>
      <field name="res_model">nse.demo</field>
      <field name="view_mode">tree,form</field>
      <field name="context">{}</field>
      <field name="help" type="html">
          <p class="o_view_nocontent_smiling_face">
            Add a new nse. Everyone deserves a Nse
          </p>
        </field>
    </record>

    <menuitem
      id="menu_nse"
      name="NSE"
      sequence="15"
      action="nse_1.action_nse"
    />


</odoo>
