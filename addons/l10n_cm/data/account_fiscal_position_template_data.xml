<odoo>
    <data noupdate="1">
        <record id="fiscal_position_national" model="account.fiscal.position.template">
            <field name="name">National</field>
            <field name="auto_apply" eval="True"/>
            <field name="sequence">10</field>
            <field name="country_id" ref="base.cm"/>
            <field name="chart_template_id" ref="l10n_syscohada.syscohada_chart_template"/>
        </record>
        <record id="fiscal_position_international" model="account.fiscal.position.template">
            <field name="name">International</field>
            <field name="auto_apply" eval="True"/>
            <field name="sequence">20</field>
            <field name="chart_template_id" ref="l10n_syscohada.syscohada_chart_template"/>
        </record>



        <record id="export_sale_19_25" model="account.fiscal.position.tax.template">
            <field name="position_id" ref="fiscal_position_international" />
            <field name="tax_src_id" ref="tva_sale_19_25"/>
            <field name="tax_dest_id" ref="tva_export_0"/>
        </record>

        <record id="export_exonere_0" model="account.fiscal.position.tax.template">
            <field name="position_id" ref="fiscal_position_international" />
            <field name="tax_src_id" ref="tva_exonere_0"/>
            <field name="tax_dest_id" ref="tva_export_0"/>
        </record>

        <record id="import_purchase_19_25" model="account.fiscal.position.tax.template">
            <field name="position_id" ref="fiscal_position_international"/>
            <field name="tax_src_id" ref="tva_purchase_19_25"/>
            <field name="tax_dest_id" ref="tva_import_19_25"/>
        </record>

        <record id="import_purchase_services_19_25" model="account.fiscal.position.tax.template">
            <field name="position_id" ref="fiscal_position_international"/>
            <field name="tax_src_id" ref="tva_purchase_services_19_25"/>
            <field name="tax_dest_id" ref="tva_import_services_19_25"/>
        </record>
    </data>
</odoo>
