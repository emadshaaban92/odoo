<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="tax_report" model="account.tax.report">
        <field name="name">Tax Report</field>
        <field name="country_id" ref="base.cm"/>
    </record>

    <record id="tax_report_op_realisees" model="account.tax.report.line">
        <field name="name">A. Opérations réalisées</field>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">10</field>
    </record>

    <record id="tax_report_op_realisee_chiffre_affaires" model="account.tax.report.line">
        <field name="name">Chiffre d'affaires</field>
        <field name="parent_id" ref="tax_report_op_realisees"/>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">20</field>
    </record>

    <record id="tax_report_op_realisee_chiffre_affaires_tax_normal" model="account.tax.report.line">
        <field name="name">Opérations taxables au taux normal</field>
        <field name="tag_name">L10_a</field>
        <field name="code">L10_a</field>
        <field name="parent_id" ref="tax_report_op_realisee_chiffre_affaires"/>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">30</field>
    </record>

    <record id="tax_report_op_realisee_chiffre_affaires_tax_autre" model="account.tax.report.line">
        <field name="name">Autres opérations taxables</field>
        <field name="tag_name">L12_a</field>
        <field name="code">L12_a</field>
        <field name="parent_id" ref="tax_report_op_realisee_chiffre_affaires"/>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">40</field>
    </record>

    <record id="tax_report_op_realisee_chiffre_affaires_export" model="account.tax.report.line">
        <field name="name">Exportations</field>
        <field name="tag_name">L13_a</field>
        <field name="code">L13_a</field>
        <field name="parent_id" ref="tax_report_op_realisees"/>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">50</field>
    </record>

    <record id="tax_report_op_realisees_tva_collectee" model="account.tax.report.line">
        <field name="name">TVA collectée</field>
        <field name="parent_id" ref="tax_report_op_realisees"/>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">60</field>
    </record>

    <record id="tax_report_tva_normal" model="account.tax.report.line">
        <field name="name">Taux normal</field>
        <field name="tag_name">L10_b</field>
        <field name="code">L10_b</field>
        <field name="parent_id" ref="tax_report_op_realisees_tva_collectee"/>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">70</field>
    </record>

    <record id="tax_report_tva_autre" model="account.tax.report.line">
        <field name="name">Autres opérations taxables</field>
        <field name="tag_name">L12_b</field>
        <field name="code">L12_b</field>
        <field name="parent_id" ref="tax_report_op_realisees_tva_collectee"/>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">80</field>
    </record>

    <record id="tax_report_deductions" model="account.tax.report.line">
        <field name="name">B. TVA déductible</field>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">90</field>
    </record>

    <record id="tax_report_deductions_achat_local" model="account.tax.report.line">
        <field name="name">Achats Locaux</field>
        <field name="tag_name">L18</field>
        <field name="code">L18</field>
        <field name="parent_id" ref="tax_report_deductions"/>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">100</field>
    </record>

    <record id="tax_report_deductions_service_local" model="account.tax.report.line">
        <field name="name">Services locaux</field>
        <field name="tag_name">L19</field>
        <field name="code">L19</field>
        <field name="parent_id" ref="tax_report_deductions"/>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">110</field>
    </record>

    <record id="tax_report_deductions_achat_import" model="account.tax.report.line">
        <field name="name">Achats à l'étranger</field>
        <field name="tag_name">L20</field>
        <field name="code">L20</field>
        <field name="parent_id" ref="tax_report_deductions"/>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">120</field>
    </record>

    <record id="tax_report_deductions_services_import" model="account.tax.report.line">
        <field name="name">Services à l'étranger</field>
        <field name="tag_name">L21</field>
        <field name="code">L21</field>
        <field name="parent_id" ref="tax_report_deductions"/>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">130</field>
    </record>
</odoo>