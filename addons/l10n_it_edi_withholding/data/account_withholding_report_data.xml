<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <record id="withh_tax_report_it" model="account.report">
        <field name="name">Withholding Report</field>
        <field name="root_report_id" ref="account.generic_tax_report"/>
        <field name="country_id" ref="base.it"/>
        <field name="column_ids">
            <record id="withh_tax_report_balance" model="account.report.column">
                <field name="name">Total</field>
                <field name="expression_label">total</field>
            </record>
        </field>
        <field name="line_ids">
            <record id="withh_tax_report_it_line" model="account.report.line">
                <field name="name">Withholding Amount</field>
                <field name="code">wth</field>
                <field name="expression_ids">
                    <record id="withh_tax_report_it_line_tag" model="account.report.expression">
                        <field name="label">total</field>
                        <field name="engine">tax_tags</field>
                        <field name="formula">WTH</field>
                    </record>
                </field>
            </record>
            <record id="enasarco_tax_report_it_line" model="account.report.line">
                <field name="name">ENASARCO Amount</field>
                <field name="code">enasarco</field>
                <field name="expression_ids">
                    <record id="enasarco_tax_report_it_line_tag" model="account.report.expression">
                        <field name="label">total</field>
                        <field name="engine">tax_tags</field>
                        <field name="formula">ENASARCO</field>
                    </record>
                </field>
            </record>
        </field>
    </record>
</odoo>
