<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="tax_report" model="account.report">
        <field name="name">Tax Report</field>
        <field name="root_report_id" ref="account.generic_tax_report"/>
        <field name="country_id" ref="base.si"/>
        <field name="filter_fiscal_position" eval="True"/>
        <field name="availability_condition">country</field>
        <field name="column_ids">
            <record id="tax_report_balance" model="account.report.column">
                <field name="name">Balance</field>
                <field name="expression_label">balance</field>
            </record>
        </field>
        <field name="line_ids">
            <record id="tax_report_zo_dvd" model="account.report.line">
                <field name="name">Znesek osnov za DDV</field>
                <field name="aggregation_formula">ZO_NEODBITNI_DDV.balance + ZO_ODBITNI_DDV.balance</field>
                <field name="children_ids">
                    <record id="tax_report_zo_dvd_neo" model="account.report.line">
                        <field name="name">Neodbitni DDV</field>
                        <field name="code">ZO_NEODBITNI_DDV</field>
                        <field name="aggregation_formula">ZO_NEODBITNI_VSTOPNI_DDV.balance</field>
                        <field name="children_ids">
                            <record id="tax_report_zo_dvd_neo_sdtopni" model="account.report.line">
                                <field name="name">Vstopni DDV</field>
                                <field name="code">ZO_NEODBITNI_VSTOPNI_DDV</field>
                                <field name="aggregation_formula">NABAVE_PO_OSNOVNI_STOPNJI_DDV_NEODBITNI.balance + NABAVE_PO_ZNIZANI_STOPNJI_DDV_NEODBITNI.balance</field>
                                <field name="children_ids">
                                    <record id="tax_report_zo_dvd_neo_sdtopni_osno" model="account.report.line">
                                        <field name="name">Nabave po osnovni stopnji DDV</field>
                                        <field name="code">NABAVE_PO_OSNOVNI_STOPNJI_DDV_NEODBITNI</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zo_dvd_neo_sdtopni_osno_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Nabave po osnovni stopnji DDV (Neodbitni)</field>
                                            </record>
                                        </field>
                                    </record>
                                    <record id="tax_report_zo_dvd_neo_sdtopni_zni" model="account.report.line">
                                        <field name="name">Nabave po znižani stopnji DDV</field>
                                        <field name="code">NABAVE_PO_ZNIZANI_STOPNJI_DDV_NEODBITNI</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zo_dvd_neo_sdtopni_zni_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Nabave po znižani stopnji DDV (Neodbitni)</field>
                                            </record>
                                        </field>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>
                    <record id="tax_report_zo_dvd_odb" model="account.report.line">
                        <field name="name">Odbitni DDV</field>
                        <field name="code">ZO_ODBITNI_DDV</field>
                        <field name="aggregation_formula">ZO_IZSTOPNI_DDV.balance + ODBITNI_VSTOPNI_DDV.balance</field>
                        <field name="children_ids">
                            <record id="tax_report_zo_dvd_odb_izstopni" model="account.report.line">
                                <field name="name">Izstopni DDV</field>
                                <field name="code">ZO_IZSTOPNI_DDV</field>
                                <field name="aggregation_formula">PRODAJA_OPROSCENA_DDV.balance + PRODAJA_PO_OSNOVNI_STOPNJI_DDV.balance + PRODAJA_PO_ZNIZANI_STOPNJI_DDV.balance</field>
                                <field name="children_ids">
                                    <record id="tax_report_zo_dvd_odb_izstopni_opr" model="account.report.line">
                                        <field name="name">Prodaja oproščena DDV</field>
                                        <field name="code">PRODAJA_OPROSCENA_DDV</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zo_dvd_odb_izstopni_opr_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Prodaja oproščena DDV</field>
                                            </record>
                                        </field>
                                    </record>
                                    <record id="tax_report_zo_dvd_odb_izstopni_osn" model="account.report.line">
                                        <field name="name">Prodaja po osnovni stopnji DDV</field>
                                        <field name="code">PRODAJA_PO_OSNOVNI_STOPNJI_DDV</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zo_dvd_odb_izstopni_osn_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Prodaja po osnovni stopnji DDV</field>
                                            </record>
                                        </field>
                                    </record>
                                    <record id="tax_report_zo_dvd_odb_izstopni_zni" model="account.report.line">
                                        <field name="name">Prodaja po znižani stopnji DDV</field>
                                        <field name="code">PRODAJA_PO_ZNIZANI_STOPNJI_DDV</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zo_dvd_odb_izstopni_zni_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Prodaja po znižani stopnji DDV</field>
                                            </record>
                                        </field>
                                    </record>
                                </field>
                            </record>
                            <record id="tax_report_zo_dvd_odb_vstopni" model="account.report.line">
                                <field name="name">Vstopni DDV</field>
                                <field name="code">ODBITNI_VSTOPNI_DDV</field>
                                <field name="aggregation_formula">NABAVE_OPROSCENE_DDV.balance + NABAVE_PO_OSNOVNI_STOPNJI_DDV_VSTOPNI.balance + NABAVE_PO_ZNIZANI_STOPNJI_DDV_VSTOPNI.balance</field>
                                <field name="children_ids">
                                    <record id="tax_report_zo_dvd_odb_vstopni_opr" model="account.report.line">
                                        <field name="name">Nabave oproščene DDV</field>
                                        <field name="code">NABAVE_OPROSCENE_DDV</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zo_dvd_odb_vstopni_opr_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Nabave oproščene DDV</field>
                                            </record>
                                        </field>
                                    </record>
                                    <record id="tax_report_zo_dvd_odb_vstopni_osn" model="account.report.line">
                                        <field name="name">Nabave po osnovni stopnji DDV</field>
                                        <field name="code">NABAVE_PO_OSNOVNI_STOPNJI_DDV_VSTOPNI</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zo_dvd_odb_vstopni_osn_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Nabave po osnovni stopnji DDV (Vstopni)</field>
                                            </record>
                                        </field>
                                    </record>
                                    <record id="tax_report_zo_dvd_odb_vstopni_zni" model="account.report.line">
                                        <field name="name">Nabave po znižani stopnji DDV</field>
                                        <field name="code">NABAVE_PO_ZNIZANI_STOPNJI_DDV_VSTOPNI</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zo_dvd_odb_vstopni_zni_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Nabave po znižani stopnji DDV (Vstopni)</field>
                                            </record>
                                        </field>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>
                </field>
            </record>
            <record id="tax_report_zn_dvd" model="account.report.line">
                <field name="name">Znesek DDV</field>
                <field name="aggregation_formula">ZN_NEODBITNI_DDV.balance + ZN_ODBITNI_DDV.balance</field>
                <field name="children_ids">
                    <record id="tax_report_zn_dvd_neo" model="account.report.line">
                        <field name="name">Neodbitni DDV</field>
                        <field name="code">ZN_NEODBITNI_DDV</field>
                        <field name="aggregation_formula">ZN_NEODBITNI_VSTOPNI_DDV.balance</field>
                        <field name="children_ids">
                            <record id="tax_report_zn_dvd_neo_vstopni" model="account.report.line">
                                <field name="name">Vstopni DDV</field>
                                <field name="code">ZN_NEODBITNI_VSTOPNI_DDV</field>
                                <field name="aggregation_formula">NEODBITNI_DDV__OSNOVNA_STOPNJA.balance + NEODBITNI_DDV__ZNIZANA_STOPNJA.balance</field>
                                <field name="children_ids">
                                    <record id="tax_report_zn_dvd_neo_vstopni_osnnovna" model="account.report.line">
                                        <field name="name">Neodbitni DDV – osnovna stopnja</field>
                                        <field name="code">NEODBITNI_DDV__OSNOVNA_STOPNJA</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zn_dvd_neo_vstopni_osnnovna_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Neodbitni DDV – osnovna stopnja</field>
                                            </record>
                                        </field>
                                    </record>
                                    <record id="tax_report_zn_dvd_neo_vstopni_zni" model="account.report.line">
                                        <field name="name">Neodbitni DDV – znižana stopnja</field>
                                        <field name="code">NEODBITNI_DDV__ZNIZANA_STOPNJA</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zn_dvd_neo_vstopni_zni_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Neodbitni DDV – znižana stopnja</field>
                                            </record>
                                        </field>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>
                    <record id="tax_report_zn_dvd_odb" model="account.report.line">
                        <field name="name">Odbitni DDV</field>
                        <field name="code">ZN_ODBITNI_DDV</field>
                        <field name="aggregation_formula">ZN_IZSTOPNI_DDV.balance + VSTOPNI_DDV.balance</field>
                        <field name="children_ids">
                            <record id="tax_report_zn_dvd_odb_izstopni" model="account.report.line">
                                <field name="name">Izstopni DDV</field>
                                <field name="code">ZN_IZSTOPNI_DDV</field>
                                <field name="aggregation_formula">IZSTOPNI_DDV__OSNOVNA_STOPNJA.balance + IZSTOPNI_DDV__ZNIZANA_STOPNJA.balance</field>
                                <field name="children_ids">
                                    <record id="tax_report_zn_dvd_odb_izstopni_osn" model="account.report.line">
                                        <field name="name">Izstopni DDV - osnovna stopnja</field>
                                        <field name="code">IZSTOPNI_DDV__OSNOVNA_STOPNJA</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zn_dvd_odb_izstopni_osn_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Izstopni DDV - osnovna stopnja</field>
                                            </record>
                                        </field>
                                    </record>
                                    <record id="tax_report_zn_dvd_odb_izstopni_zni" model="account.report.line">
                                        <field name="name">Izstopni DDV - znižana stopnja</field>
                                        <field name="code">IZSTOPNI_DDV__ZNIZANA_STOPNJA</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zn_dvd_odb_izstopni_zni_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Izstopni DDV - znižana stopnja</field>
                                            </record>
                                        </field>
                                    </record>
                                </field>
                            </record>
                            <record id="tax_report_zn_dvd_odb_vstopni" model="account.report.line">
                                <field name="name">Vstopni DDV</field>
                                <field name="code">VSTOPNI_DDV</field>
                                <field name="aggregation_formula">VSTOPNI_DDV__OSNOVNA_STOPNJA.balance + VSTOPNI_DDV__ZNIZANA_STOPNJA.balance</field>
                                <field name="children_ids">
                                    <record id="tax_report_zn_dvd_odb_vstopni_osn" model="account.report.line">
                                        <field name="name">Vstopni DDV - osnovna stopnja</field>
                                        <field name="code">VSTOPNI_DDV__OSNOVNA_STOPNJA</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zn_dvd_odb_vstopni_osn_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Vstopni DDV - osnovna stopnja</field>
                                            </record>
                                        </field>
                                    </record>
                                    <record id="tax_report_zn_dvd_odb_vstopni_zni" model="account.report.line">
                                        <field name="name">Vstopni DDV - znižana stopnja</field>
                                        <field name="code">VSTOPNI_DDV__ZNIZANA_STOPNJA</field>
                                        <field name="expression_ids">
                                            <record id="tax_report_zn_dvd_odb_vstopni_zni_tag" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">tax_tags</field>
                                                <field name="formula">Vstopni DDV - znižana stopnja</field>
                                            </record>
                                        </field>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>
                </field>
            </record>
        </field>
    </record>
</odoo>
