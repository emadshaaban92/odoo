        <record id="tax_report_15" model="account.tax.report.line">
            <field name="name">15. Exempt supplies without the right to deduct VAT</field>
            <field name="code">c15</field>
            <field name="tag_name">15</field>
            <field name="report_id" ref="tax_report"/>
            <field name="parent_id" ref="tax_report_I"/>
            <field name="sequence">1150</field>
        </record>
    <!-- I. Supplies of goods and services (values excluding VAT) [END] -->

    <!-- II. VAT charged -->
	<record id="tax_report_II" model="account.tax.report.line">
        <field name="name">II. VAT charged</field>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence">2000</field>
    </record>
        <record id="tax_report_21" model="account.tax.report.line">
