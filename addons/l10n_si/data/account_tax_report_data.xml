            <field name="code">c21</field>
            <field name="tag_name">21</field>
            <field name="report_id" ref="tax_report"/>
            <field name="parent_id" ref="tax_report_II"/>
            <field name="sequence">2210</field>
        </record>

        <record id="tax_report_22" model="account.tax.report.line">
            <field name="name">22. At a rate of 9,5%</field>
