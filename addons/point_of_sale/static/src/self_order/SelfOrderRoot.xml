<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="SelfOrderRoot" owl="1">
            <t t-if="state.selfOrderCurrentScreen == 0">
                <!-- <LandingPageStart tableNumber="tableNumber" currentScreen="state.selfOrderCurrentScreen"/> -->
                <LandingPageStart tableNumber="tableNumber" state="this.state"/>
            </t>
            <t t-elif="state.selfOrderCurrentScreen == 1">
                <!-- <ProductList restaurantName="restaurantName" tableNumber="tableNumber" productList="productList"/> -->
                <ProductList restaurantName="restaurantName" tableNumber="tableNumber" productList="productList"/>
            </t>
            <t t-elif="state.selfOrderCurrentScreen == 2">
                <ProductMainView restaurantName="restaurantName" tableNumber="tableNumber"/>
            </t>
            <t t-elif="state.selfOrderCurrentScreen == 3">
                <LandingPageEnd tableNumber="tableNumber" message="message"/>
            </t>
    </t>
</templates>

