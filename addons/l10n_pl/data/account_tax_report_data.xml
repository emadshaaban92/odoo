        <field name="tag_name">K_45</field>
        <field name="code">PLTAXD_02_45</field>
        <field name="report_id" ref="tax_report"/>
        <field name="sequence" eval="6"/>
        <field name="parent_id" ref="account_tax_report_line_podatek_razem_d"/>
