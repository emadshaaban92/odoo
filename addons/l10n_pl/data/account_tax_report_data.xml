        <field name="parent_id" ref="account_tax_report_line_razem_c"/>
    </record>

    <record id="account_tax_report_line_kraj_7_lub_8" model="account.tax.report.line">
        <field name="name">Podstawa - Dostawa towarów/usług, kraj, 7% lub 8%</field>
