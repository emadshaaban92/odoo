<?xml version="1.0" encoding="utf-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="stock.ReplenishReport" t-inherit="web.ReportAction" t-inherit-mode="primary" owl="1" >
        <xpath expr="//button[@title='Print']" position="attributes">
            <attribute name="t-if">false</attribute>
        </xpath>
        <xpath expr="//t/Layout/t" position="inside">
            <ReplenishReportButtons action="props.action" onReplenish.bind="reloadReport"/>
            <div class="o_cp_bottom_right">
                <div class="btn-group o_search_options position-static" role="search">
                    <WarehouseFilter action="props.action" onSelected.bind="reloadReport" updateContext.bind="updateContext"/>
                </div>
            </div>
        </xpath>
        <!--xpath expr="//t/Layout" position="after">
            <GraphView/>
        </xpath-->
    </t>
</templates>
