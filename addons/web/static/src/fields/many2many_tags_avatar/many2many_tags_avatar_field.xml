<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="web.Many2ManyTagsAvatarField" owl="1">
        <TagsList tags="tags" itemsVisible="props.itemsVisible">
            <div t-if="showM2OSelectionField" class="o_field_many2many_selection">
                <Many2XAutocomplete
                    id="props.id"
                    placeholder="props.placeholder"
                    resModel="props.relation"
                    autoSelect="true"
                    fieldString="fieldString"
                    activeActions="activeActions"
                    update="update"
                    createRecord="createRecord"
                    context="context"
                    getDomain.bind="getDomain"
                    isToMany="true"
                />
            </div>
        </TagsList>
    </t>

</templates>
