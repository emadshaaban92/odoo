<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="l10n_ke_view_company_form_inherit" model="ir.ui.view">
        <field name="name">l10n.ke.view.company.form</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="model">res.company</field>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="EDI details" name="edi_details">
                    <group string="Kenyan Fiscal Device">
                        <group>
                            <field name="l10n_ke_device_sender_id"/>
                            <field name="l10n_ke_device_proxy_url"/>
                            <field name="l10n_ke_device_url"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
