                        <field name="engine">aggregation</field>
                        <field name="formula">max(0, tax_report_pt_tax_vendas_faturas.balance - tax_report_pt_tax_vendas_notas_credito.balance - tax_report_pt_tax_despesas_faturas.balance + tax_report_pt_tax_despesas_notas_credito).balance</field>
