<?xml version="1.0"?>
<odoo>
    <!-- <data noupdate="1"> -->
        <record id="pos_category_miscellaneous" model="pos.category">
            <field name="name">Miscellaneous</field>
        </record>

        <!-- Kitchen Printer -->

        <record id="kitchen_printer" model="restaurant.printer">
            <field name="name">Kitchen Printer</field>
            <field name="proxy_ip">localhost</field>
            <field name="product_categories_ids" eval="[(6, 0, [ref('pos_restaurant.pos_category_miscellaneous')])]" />
        </record>

        <record id="product_category_pos_food" model="product.category">
            <field name="parent_id" ref="point_of_sale.product_category_pos"/>
            <field name="name">Food</field>
        </record>

        <record id="food" model="pos.category">
            <field name="name">Food</field>
        </record>

        <!-- Template -->
        <record id="pizza_product_template" model="product.template">
            <field name="name">Pizza Template</field>
        </record>

        <!-- Attributes  -->
        <record id="truffle_oil_pizza_product_attribute" model="product.attribute">
            <field name="name">Truffle Oil</field>
            <field name="sequence">1</field>
            <field name="display_type">pills</field>
            <field name="create_variant">no_variant</field>
        </record>
        <record id="mozzarella_pizza_product_attribute" model="product.attribute">
            <field name="name">Mozzarella di Buffala</field>
            <field name="sequence">2</field>
            <field name="display_type">pills</field>
            <field name="create_variant">no_variant</field>
        </record>
        <record id="prosciutto_pizza_product_attribute" model="product.attribute">
            <field name="name">Prosciutto Crudo</field>
            <field name="sequence">3</field>
            <field name="display_type">pills</field>
            <field name="create_variant">no_variant</field>
        </record>

        <!-- Attribute Values -->
        <record id="truffle_oil_topping_product_attribute_value_yes" model="product.attribute.value">
            <field name="name">Truffle Oil Topping</field>
            <field name="attribute_id" ref="truffle_oil_pizza_product_attribute"/>
        </record>
        <record id="truffle_oil_topping_product_attribute_value_no" model="product.attribute.value">
            <field name="name">No Truffle Oil Topping</field>
            <field name="attribute_id" ref="truffle_oil_pizza_product_attribute"/>
        </record>

        <record id="mozzarella_topping_product_attribute_value_yes" model="product.attribute.value">
            <field name="name">Mozzarella</field>
            <field name="attribute_id" ref="mozzarella_pizza_product_attribute"/>
        </record>
        <record id="mozzarella_topping_product_attribute_value_no" model="product.attribute.value">
            <field name="name">No Mozzarella</field>
            <field name="attribute_id" ref="mozzarella_pizza_product_attribute"/>
        </record>
        <record id="prosciutto_topping_product_attribute_value_yes" model="product.attribute.value">
            <field name="name">Prosciutto Topping</field>
            <field name="attribute_id" ref="prosciutto_pizza_product_attribute"/>
        </record>
        <record id="prosciutto_topping_product_attribute_value_no" model="product.attribute.value">
            <field name="name">No Prosciutto Topping</field>
            <field name="attribute_id" ref="prosciutto_pizza_product_attribute"/>
        </record>

        <!-- Attribute Lines -->
        <record id="truffle_oil_template_attribute_line" model="product.template.attribute.line">
            <field name="product_tmpl_id" ref="pos_restaurant.pizza_product_template"/>
            <field name="attribute_id" ref="pos_restaurant.truffle_oil_pizza_product_attribute"/>
            <field name="value_ids" eval="[(6,0,[ref('pos_restaurant.truffle_oil_topping_product_attribute_value_yes'), ref('pos_restaurant.truffle_oil_topping_product_attribute_value_no')])]"/>
        </record>
        <record id="mozzarella_template_attribute_line" model="product.template.attribute.line">
            <field name="product_tmpl_id" ref="pos_restaurant.pizza_product_template"/>
            <field name="attribute_id" ref="pos_restaurant.mozzarella_pizza_product_attribute"/>
            <field name="value_ids" eval="[(6,0,[ref('pos_restaurant.mozzarella_topping_product_attribute_value_yes'), ref('pos_restaurant.mozzarella_topping_product_attribute_value_no')])]"/>
        </record>
        <record id="prosciutto_template_attribute_line" model="product.template.attribute.line">
            <field name="product_tmpl_id" ref="pos_restaurant.pizza_product_template"/>
            <field name="attribute_id" ref="pos_restaurant.prosciutto_pizza_product_attribute"/>
            <field name="value_ids" eval="[(6,0,[ref('pos_restaurant.prosciutto_topping_product_attribute_value_yes'), ref('pos_restaurant.prosciutto_topping_product_attribute_value_no')])]"/>
        </record>

        <!-- Products -->
        <record id="pos_food_pim" model="product.template">
            <field name="available_in_pos">True</field>
            <field name="list_price">7.0</field>
            <field name="name">PimPizza</field>
            <field name="pos_categ_id" ref="food"/>
            <field name="categ_id" ref="pos_restaurant.product_category_pos_food"/>
            <field name="image_1920" type="base64" file="pos_restaurant/static/img/th-pizza.png"/>
            <field name="attribute_line_ids" eval="[(0,0,{
                'attribute_id':ref('pos_restaurant.truffle_oil_pizza_product_attribute'),
                'value_ids': [(6,0,[ref('pos_restaurant.truffle_oil_topping_product_attribute_value_yes'), ref('pos_restaurant.truffle_oil_topping_product_attribute_value_no')])],
                'product_template_value_ids': [(0,0, {
                    'product_attribute_value_id': ref('pos_restaurant.truffle_oil_topping_product_attribute_value_yes'),
                    'price_extra': 3.0,
                    })],
                }),
                (0,0,{
                'attribute_id':ref('pos_restaurant.mozzarella_pizza_product_attribute'),
                'value_ids': [(6,0,[ref('pos_restaurant.mozzarella_topping_product_attribute_value_yes'), ref('pos_restaurant.mozzarella_topping_product_attribute_value_no')])],
                'product_template_value_ids': [(0,0, {
                    'product_attribute_value_id': ref('pos_restaurant.mozzarella_topping_product_attribute_value_yes'),
                    'price_extra': 3.0,
                    })],
                }),
                (0,0,{
                'attribute_id':ref('pos_restaurant.prosciutto_pizza_product_attribute'),
                'value_ids': [(6,0,[ref('pos_restaurant.prosciutto_topping_product_attribute_value_yes'), ref('pos_restaurant.prosciutto_topping_product_attribute_value_no')])],
                'product_template_value_ids': [(0,0, {
                    'product_attribute_value_id': ref('pos_restaurant.prosciutto_topping_product_attribute_value_yes'),
                    'price_extra': 3.0,
                    })],
                })]"/>
        </record>

