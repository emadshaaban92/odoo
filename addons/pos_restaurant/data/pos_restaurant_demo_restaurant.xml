            <field name="iface_orderline_notes">True</field>
            <field name="printer_ids" eval="[(6, 0, [ref('pos_restaurant.kitchen_printer')])]" />
            <field name="iface_start_categ_id" ref="food"/>
            <field name="start_category">True</field>
        </record>

