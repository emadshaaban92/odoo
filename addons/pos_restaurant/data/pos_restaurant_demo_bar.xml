<?xml version="1.0"?>
<odoo>
    <data noupdate="1">
        <record id="pos_category_miscellaneous" model="pos.category">
            <field name="name">Miscellaneous</field>
        </record>

        <!-- Bar Printer -->
        <record id="bar_printer" model="restaurant.printer">
            <field name="name">Bar Printer</field>
            <field name="proxy_ip">localhost</field>
            <field name="product_categories_ids" eval="[(6, 0, [ref('pos_restaurant.pos_category_miscellaneous')])]" />
        </record>

        <record id="product_category_pos_drinks" model="product.category">
            <field name="parent_id" ref="point_of_sale.product_category_pos"/>
            <field name="name">Drinks</field>
        </record>

        <record id="drinks" model="pos.category">
            <field name="name">Drinks</field>
        </record>

        <!-- Drinks -->
        <record id="pos_drink_mojito" model="product.product">
            <field name="available_in_pos">True</field>
            <field name="list_price">7.0</field>
            <field name="name">Mojito</field>
            <field name="pos_categ_id" ref="drinks"/>
            <field name="categ_id" ref="pos_restaurant.product_category_pos_drinks"/>
            <field name="image_1920" type="base64" file="pos_restaurant/static/img/mojito.png"/>
        </record>
        <record id="pos_drink_gin_tonic" model="product.product">
            <field name="available_in_pos">True</field>
            <field name="list_price">7.50</field>
            <field name="name">Gin Tonic</field>
            <field name="pos_categ_id" ref="drinks"/>
            <field name="categ_id" ref="pos_restaurant.product_category_pos_drinks"/>
            <field name="image_1920" type="base64" file="pos_restaurant/static/img/gin_tonic.png"/>
        </record>
        <record id="pos_drink_moscow_mule" model="product.product">
            <field name="available_in_pos">True</field>
            <field name="list_price">8.50</field>
            <field name="name">Moscow Mule</field>
            <field name="pos_categ_id" ref="drinks"/>
            <field name="categ_id" ref="pos_restaurant.product_category_pos_drinks"/>
            <field name="image_1920" type="base64" file="pos_restaurant/static/img/moscow_mule.jpg"/>
        </record>
        <record id="pos_drink_spritz" model="product.product">
            <field name="available_in_pos">True</field>
            <field name="list_price">6.50</field>
            <field name="name">Virgin Spritz</field>
            <field name="pos_categ_id" ref="drinks"/>
            <field name="categ_id" ref="pos_restaurant.product_category_pos_drinks"/>
            <field name="image_1920" type="base64" file="pos_restaurant/static/img/spritz.png"/>
        </record>
        <record id="pos_drink_strawberry_daikiri" model="product.product">
            <field name="available_in_pos">True</field>
            <field name="list_price">9.0</field>
            <field name="name">Strawberry Daikiri</field>
            <field name="pos_categ_id" ref="drinks"/>
            <field name="categ_id" ref="pos_restaurant.product_category_pos_drinks"/>
            <field name="image_1920" type="base64" file="pos_restaurant/static/img/strawberry_daikiri.png"/>
        </record>

        <!-- Pos Config -->
        <record model="pos.config" id="pos_config_main">
            <field name="name">Bar</field>
