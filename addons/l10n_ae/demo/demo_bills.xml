    </record>


    <function model="account.payment" name="action_post">
        <value eval="[ref('demo_payment_bill_ae_1')]" />
    </function>

    <function model="account.payment" name="action_post">
        <value eval="[ref('demo_payment_bill_ae_2')]" />
    </function>

    <function model="account.payment" name="action_post">
        <value eval="[ref('demo_payment_bill_ae_3')]" />
    </function>

</odoo>
