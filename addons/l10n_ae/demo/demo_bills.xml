<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="True">

    <record id="demo_bill_ae_1" model="account.move">
        <field name="partner_id" ref="partner_demo_four_style_carpentry" />
        <field name="invoice_user_id" ref="base.user_demo" />
        <field name="move_type">in_invoice</field>
        <field name="invoice_date">2022-07-12</field>
        <field name="company_id" ref="demo_company_ae" />
        <field name="invoice_payment_term_id" ref="account.account_payment_term_end_following_month" />
        <field name="invoice_line_ids" eval="[
            (0, 0, {'product_id': ref('demo_product_ae_4'), 'price_unit': 12.0, 'quantity': 3.0}),
            (0, 0, {'product_id': ref('demo_product_ae_5'), 'price_unit': 22.0, 'quantity': 12.0}),

        ]" />
    </record>

    <record id="demo_bill_ae_2" model="account.move">
        <field name="move_type">in_invoice</field>
        <field name="partner_id" ref="partner_demo_trading_paints" />
        <field name="invoice_user_id" ref="base.user_demo" />
        <field name="invoice_payment_term_id" ref="account.account_payment_term_end_following_month" />
        <field name="invoice_date">2022-07-12</field>
        <field name="company_id" ref="demo_company_ae" />
        <field name="invoice_line_ids" eval="[
        (0, 0, {'product_id': ref('demo_product_ae_4'), 'price_unit': 12.0, 'quantity': 3.0}),
        (0, 0, {'product_id': ref('demo_product_ae_5'), 'price_unit': 20.0, 'quantity': 4.0}),

    ]" />
    </record>

    <record id="demo_bill_ae_3" model="account.move">
        <field name="move_type">in_invoice</field>
        <field name="partner_id" ref="partner_demo_four_style_carpentry" />
        <field name="invoice_date">2022-07-12</field>
        <field name="invoice_user_id" ref="base.user_demo" />
        <field name="company_id" ref="demo_company_ae" />
        <field name="invoice_payment_term_id" ref="account.account_payment_term_end_following_month" />
        <field name="invoice_line_ids" eval="[
        (0, 0, {'product_id': ref('demo_product_ae_7'), 'price_unit': 40.0, 'quantity': 2.0}),
    ]" />
    </record>

    <record id="demo_payment_bill_ae_1" model="account.payment">
        <field name="partner_id" ref="partner_demo_four_style_carpentry" />
        <field name="partner_type">supplier</field>
        <field name="amount">315</field>
        <field name="payment_type">outbound</field>
        <field name="date">2022-07-12</field>
        <field name="company_id" ref="demo_company_ae" />
        <field name="journal_id" model="account.journal"
            eval="obj().search([
                ('type', '=', 'bank'),
                ('company_id', '=', ref('demo_company_ae'))], limit=1).id" />
    </record>

    <record id="demo_payment_bill_ae_2" model="account.payment">
        <field name="partner_id" ref="partner_demo_trading_paints" />
        <field name="partner_type">supplier</field>
        <field name="amount">121.80</field>
        <field name="payment_type">outbound</field>
        <field name="date">2022-07-12</field>
        <field name="company_id" ref="demo_company_ae" />
        <field name="journal_id" model="account.journal"
            eval="obj().search([
                ('type', '=', 'bank'),
                ('company_id', '=', ref('demo_company_ae'))], limit=1).id" />
    </record>

    <record id="demo_payment_bill_ae_3" model="account.payment">
        <field name="partner_id" ref="partner_demo_four_style_carpentry" />
        <field name="partner_type">supplier</field>
