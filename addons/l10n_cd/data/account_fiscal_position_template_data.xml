<odoo>
    <data noupdate="1">
        <record id="fiscal_position_national" model="account.fiscal.position.template">
            <field name="name">National</field>
            <field name="auto_apply" eval="True"/>
            <field name="sequence">10</field>
            <field name="country_id" ref="base.cd"/>
            <field name="chart_template_id" ref="l10n_syscohada.syscohada_chart_template"/>
        </record>
        <record id="fiscal_position_international" model="account.fiscal.position.template">
            <field name="name">International</field>
            <field name="auto_apply" eval="True"/>
            <field name="sequence">20</field>
            <field name="chart_template_id" ref="l10n_syscohada.syscohada_chart_template"/>
        </record>


        <record id="export" model="account.fiscal.position.tax.template">
            <field name="position_id" ref="fiscal_position_international"/>
            <field name="tax_src_id" ref="tva_sale_16"/>
            <field name="tax_dest_id" ref="tva_export_0"/>
        </record>

        <record id="export_services" model="account.fiscal.position.tax.template">
            <field name="position_id" ref="fiscal_position_international"/>
            <field name="tax_src_id" ref="tva_services_16"/>
            <field name="tax_dest_id" ref="tva_export_0"/>
        </record>

        <record id="export_sale_public" model="account.fiscal.position.tax.template">
            <field name="position_id" ref="fiscal_position_international"/>
            <field name="tax_src_id" ref="tva_sale_public_16"/>
            <field name="tax_dest_id" ref="tva_export_0"/>
        </record>

        <record id="importation_marchandises" model="account.fiscal.position.tax.template">
            <field name="position_id" ref="fiscal_position_international"/>
            <field name="tax_src_id" ref="tva_purchase_marchandise_16"/>
            <field name="tax_dest_id" ref="tva_import_marchandises_16"/>
        </record>

        <record id="importation_services" model="account.fiscal.position.tax.template">
            <field name="position_id" ref="fiscal_position_international"/>
            <field name="tax_src_id" ref="tva_purchase_services_16"/>
            <field name="tax_dest_id" ref="tva_import_services_16"/>
        </record>

        <record id="importation_immobilisation" model="account.fiscal.position.tax.template">
            <field name="position_id" ref="fiscal_position_international"/>
            <field name="tax_src_id" ref="tva_purchase_immobilisation_16"/>
            <field name="tax_dest_id" ref="tva_import_immobilisation_16"/>
        </record>

        <record id="importation_matieres_premieres" model="account.fiscal.position.tax.template">
            <field name="position_id" ref="fiscal_position_international"/>
            <field name="tax_src_id" ref="tva_purchase_matieres_premieres_16"/>
            <field name="tax_dest_id" ref="tva_import_matieres_premieres_16"/>
        </record>
    </data>
</odoo>
